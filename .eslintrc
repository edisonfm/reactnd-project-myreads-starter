{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": ["error", "windows"],
    "react/jsx-filename-extension": ["off"],
    "jsx-a11y/anchor-is-valid":["warn"]
  },
  "parser": "babel-eslint"
}
